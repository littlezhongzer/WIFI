<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.quectel.quectel_stress.ShowActivity">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*"
        android:showDividers="beginning|middle|end"
        android:orientation="vertical">

    <TableRow
        android:showDividers="beginning|middle|end"
        android:orientation="horizontal">
        <TextView
            android:padding="8dp"
            android:layout_width="1dp"
            android:text="Wifi SSID"
            android:gravity="left"/>
        <TextView
            android:id="@+id/tv_show_wifi_ssid"
            android:padding="8dp"
            android:layout_width="1dp"
            android:gravity="left"/>
    </TableRow>

        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="Wifi 连接状态" />

            <TextView
                android:id="@+id/tv_show_wifi_connect_status"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="wifi开关状态" />

            <TextView
                android:id="@+id/tv_show_wifi_switcher_status"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>

        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="已测试循环数" />

            <TextView
                android:id="@+id/tv_show_round_times"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>

        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="已测试总时长" />

            <TextView
                android:id="@+id/tv_show_total_time"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>

        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="wifi打开" />

            <TextView
                android:id="@+id/tv_show_wifi_open"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="Wifi扫描" />

            <TextView
                android:id="@+id/tv_wifi_scan"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="扫描到的wifi数量" />

            <TextView
                android:id="@+id/tv_wifi_scan_wifi_nums"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="ssid_1信号强度" />

            <TextView
                android:id="@+id/tv_wifi_ssid1_level"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="ssid_2信号强度" />

            <TextView
                android:id="@+id/tv_wifi_ssid2_level"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>

        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="wifi连接" />

            <TextView
                android:id="@+id/tv_show_wifi_connect"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="Ping网络" />

            <TextView
                android:id="@+id/tv_show_wifi_ping"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="wifi断开" />

            <TextView
                android:id="@+id/tv_show_wifi_disconnect"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>
        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">

            <TextView
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                android:text="wifi关闭" />

            <TextView
                android:id="@+id/tv_show_wifi_close"
                android:layout_width="1dp"
                android:gravity="left"
                android:padding="8dp"
                />
        </TableRow>


        <TableRow
            android:orientation="horizontal"
            android:showDividers="beginning|middle|end">
        </TableRow>

    </TableLayout>

</android.support.constraint.ConstraintLayout>
